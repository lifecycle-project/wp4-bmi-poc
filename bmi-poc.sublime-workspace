			"contents": "################################################################################\n## Project: bmi-poc\n## Script purpose: Script for performing analysis\n## Date: 24.10.19\n## Author: Tim Cadman\n## Email: t.cadman@bristol.ac.uk\n################################################################################\n\nrequire(dplyr)\nrequire(magrittr)\nrequire(readstata13)\nrequire(miceadds)\nrequire(tableone)\nrequire(zscorer)\nrequire(reshape2)\n\n################################################################################\n# 1. Cohort-specific code  \n################################################################################\n\n# Note this is the only section of the code that cohorts are required to \n# make amendments to.\n\n# The only other choice cohorts need to make is which sections to run depending\n# on which exposures they have.\n\n\n## ---- Indicate form of LifeCycle data ----------------------------------------\n\n# Options are \"csv\", \"rdata\", \"stata\", e.g. \ndata_source <- \"rdata\"\n\n\n## ---- Provide paths for the lifecycle dataset --------------------------------\ndata_path <- \"z:/projects/ieu2/p6/048/working/data/wp1/wp1_data_16.09.19.RData\"\n\n\n## ---- Provide list of available exposures ------------------------------------\n\n## Delete exposures which are not available\nexposures <- c(\"edu_m_0\", \"edu_f1_0\", \"preg_dia\", \"ga_bj\")\n\n\n## ---- Provide list of available confounders ----------------------------------\n\n## Delete confounders which are not available. Note if you do not have the \n## exposure you don't need to delete the counfounder as the analysis will not\n## be run\nconfounders <- list(\n    edu_m_0 = \"sex\",\n    edu_f1_0 = \"sex\",\n    preg_dia = c(\"edu_m_0\", \"agebirth_m_y\", \"prepreg_weight\", \n                  \"height_m\", \"parity_m\", \"preg_smk\", \"sex\"),\n    ga_bj = c(\"edu_m_0\", \"ethn3_m\", \"prepreg_weight\", \"height_m\", \"preg_dia\",\n              \"preg_smk\", \"preeclam\", \"agebirth_m_y\", \"parity_m\", \"sex\"),\n    ndvi300_preg = c(\"edu_m_0\", \"ethn3_m\", \"sex\"),\n    green_dist_preg = c(\"edu_m_0\", \"ethn3_m\", \"sex\"),\n    green_size_preg = c(\"edu_m_0\", \"ethn3_m\", \"sex\"),\n    greenyn300_preg = c(\"edu_m_0\", \"ethn3_m\", \"sex\")\n    )\n\n\n# age at height/weight measurement\n\n## ---- Provide a list of available variables indicating type of measurement ---\nmeasurement <- c(\"prepreg_weight_mes\", \"height_mes_m\")\n\n\n## ---- State cohort -----------------------------------------------------------\n\n## Replace \"ALSPAC\" with your cohort name\ncohort <- \"ALSPAC\"\n\n\nexposures_mast <- c(\"edu_m_0\", \"edu_f1_0\", \"preg_dia\", \"ga_bj\", \"ndvi300_preg\",\n               \"green_dist_preg\", \"green_size_preg\", \"greenyn300_preg\")\n\nconfounders_mast <- list(\n    edu_m_0 = \"sex\",\n    edu_f1_0 = \"sex\",\n    preg_dia = c(\"edu_m_0\", \"agebirth_m_y\", \"prepreg_weight\", \n                  \"height_m\", \"parity_m\", \"preg_smk\", \"sex\"),\n    ga_bj = c(\"edu_m_0\", \"ethn3_m\", \"prepreg_weight\", \"height_m\", \"preg_dia\",\n              \"preg_smk\", \"preeclam\", \"agebirth_m_y\", \"parity_m\", \"sex\"),\n    ndvi300_preg = c(\"edu_m_0\", \"ethn3_m\", \"areases_quint_preg\", \"sex\"),\n    green_dist_preg = c(\"edu_m_0\", \"ethn3_m\", \"areases_quint_preg\", \"sex\"),\n    green_size_preg = c(\"edu_m_0\", \"ethn3_m\", \"areases_quint_preg\", \"sex\"),\n    greenyn300_preg = c(\"edu_m_0\", \"ethn3_m\", \"areases_quint_preg\", \"sex\")\n    )\n################################################################################\n# 2. Get data  \n################################################################################\n\nif(data_source == \"csv\"){\n\nbmi_data <- read.csv(data_path)\n\n} else if(data_source == \"rdata\"){\n\nload.Rdata(data_path, objname = \"bmi_data\")\n\n} else if(data_source == \"stata\"){\n\nbmi_data <- read.dta13(data_path)\n\n}\n\n\n################################################################################\n# 3. Keep only the variables we need for the analysis  \n################################################################################\n\nid_ref <- c(\"mother_id\", \"child_id\", \"cohort_id\")\n\nheight_ref <- vector()\nweight_ref <- vector()\nheight_age_ref <- vector()\nweight_age_ref <- vector()\n\nfor(i in 1:215){\nheight_ref[i] <- paste(\"height_\", i, sep = \"\")\nweight_ref[i] <- paste(\"weight_\", i, sep = \"\")\nheight_age_ref[i] <- paste(\"height_age\", i, sep = \"\")\nweight_age_ref[i] <- paste(\"weight_age\", i, sep = \"\")\n}\n\n\n## ---- Now we subset the data -------------------------------------------------\nconfounders_vec <- unique(unlist(confounders_mast, use.names = FALSE))\n\nbmi_data %<>%\nselect(one_of(exposures_mast, confounders_vec, measurement, height_ref, weight_ref,\n\t          height_age_ref, weight_age_ref, id_ref))\n\nwrite.csv(colnames(bmi_data), file = \"./bmi-poc/var_names for poc.csv\")\n\n################################################################################\n# 3. Set all data to be the correct variable types  \n################################################################################\nfactor_ref <- c(\"parity_m\", \"ethn3_m\",\"prepreg_weight_mes\", \"height_mes_m\", \n\t             \"sex\", \"preg_dia\", \"preeclam\", \"preg_smk\")\n\nbmi_data %<>%\nmutate_at(vars(one_of(factor_ref)), list(~ factor(.)))\n\n\n################################################################################\n# 3. Transform exposures  \n################################################################################\n\n## ---- Highest parental qualification -----------------------------------------\nif(all(c(\"edu_m_0\", \"edu_f1_0\") %in% exposures) == TRUE){\n\nbmi_data %<>%\nmutate(edu_par_0 = factor(pmin(edu_m_0, edu_f1_0)))\n\nexposures <- c(exposures, \"edu_par_0\")\nconfounders$edu_par_0 <- confounders$edu_m_0\n\nbmi_data %<>%\nmutate_at(vars(edu_m_0, edu_f1_0), list(~ factor(.)))\n\n} else {\n\nbmi_data %<>%\nmutate_at(vars(one_of(edu_m_0, edu_f1_0)), list(~ factor(.)))\n\n}\n\n\n## ---- Gestational age variables ----------------------------------------------\nif(\"ga_bj\" %in% exposures == TRUE){\n\nbmi_data %<>%\nmutate(ga_bj_weeks = ga_bj / 7,\n       ga_norm_pre37 = factor(ifelse(ga_bj_weeks >= 34 & ga_bj_weeks < 37, 1, 0)), \n       ga_norm_pre34 = factor(ifelse(ga_bj_weeks < 34, 1, 0)),\n       ga_norm_post42 = factor(ifelse(ga_bj_weeks > 42, 1, 0)))\n\nexposures <- c(exposures, \"ga_norm_pre37\", \"ga_norm_pre34\", \"ga_norm_post42\")\n\nconfounders$ga_norm_pre37 <- confounders$ga_bj\nconfounders$ga_norm_pre34 <- confounders$ga_bj\nconfounders$ga_norm_post42 <- confounders$ga_bj\n\n} else {}\n\n################################################################################\n# 4. Transform confounders  \n################################################################################\n\n## ---- Binary version of parity -----------------------------------------------\nif(\"parity_m\" %in% confounders_vec == TRUE){\n\nbmi_data %<>%\nmutate(parity_m = factor(ifelse(parity_m == 0, 0, 1)))\n\n} else{}\n\n## ---- Maternal BMI -----------------------------------------------------------\nif(all(c(\"prepreg_weight\", \"height_m\") %in% confounders_vec) == TRUE){\n\nbmi_data %<>%\nmutate(bmi_m = prepreg_weight/(height_m^2))\n\n} else{}\n\n\n################################################################################\n# 5. Create BMI variables  \n################################################################################\n\n## ---- First we convert to long format if data is wide ------------------------\nbmi_sub <- bmi_data %>%\nselect(child_id, height_ref, height_age_ref, weight_ref, weight_age_ref, sex)\n\nbmi_long <- reshape(data = bmi_sub,\n                    varying = list(height = height_ref, \n                    \t           height_age = height_age_ref, \n                    \t           weight = weight_ref,\n                    \t           weight_age = weight_age_ref),               \n                    direction = \"long\",\n                    idvar = \"child_id\",\n                    v.names = c(\"height\", \"height_age\", \"weight\", \"weight_age\"))\n\n\n## ---- Now we merge the age variables -----------------------------------------\nbmi_long %<>%\nmutate(age = coalesce(height_age, weight_age), \n\t   height_age = NULL,\n\t   weight_age = NULL)\n\n\n## ---- Create variable indicating age category --------------------------------\nbmi_long %<>%\nmutate(age_cat = as.factor(\n\tcase_when(\n    time <= 24 ~ \"0_2\",\n    time > 24 & time <= 48 ~ \"3_4\",\n    time > 48 & time <= 96 ~ \"5_8\",\n    time > 96 & time <= 168 ~ \"9_14\",\n    time > 168 ~ \"15_18\")))\n\n\n## ---- Drop NAs ---------------------------------------------------------------\nbmi_long %<>%\nfilter(!is.na(weight) & !is.na(height))\n\n\n## ---- Now we select one observation per subject per category -----------------\n\n## Rule is to select observation nearest the midpoint of the category\nbmi_long %<>%\ngroup_by(child_id, age_cat) %>%\nslice(ceiling(n()/2)) %>%\nungroup()\n\n## ---- Now we calculate BMI ---------------------------------------------------\n\n## Raw BMI\nbmi_long %<>% \nmutate(height_mtr = height / 100,\n\t   bmi_raw = weight/(height_mtr^2))\n\n## Internal z-score within age category\nbmi_long %<>%\ngroup_by(age_cat) %>% \nmutate(bmi_int_z = (bmi_raw - mean(bmi_raw, na.rm = TRUE)) / \n                   sd(bmi_raw, na.rm = TRUE)) %>%\nungroup()\n\n## WHO BMI for age\nbmi_long <- addWGSR(data = bmi_long, sex = \"sex\", firstPart = \"weight\",\n                    secondPart = \"height\", thirdPart = \"age\", index = \"bfa\", \n                    output = \"bmi_age_z\")\n\nbmi_long %>%\ndplyr::select(height, weight, age, bmi_age_z) %>%\nhead()\n\n\n## ---- Now we convert back to wide format -------------------------------------\ntmp <- melt(bmi_long, measure.vars = c(\"bmi_raw\", \"bmi_int_z\", \"bmi_age_z\"))\n\nbmi_wide <- dcast(tmp, formula = child_id ~ variable + age_cat, \n\t                   value.var = \"value\")\n\nbmi_wide <- merge(bmi_data, bmi_wide, by = \"child_id\")\n\n\n################################################################################\n# 6. Create descriptive statistics  \n################################################################################\n\n## ---- Exposures --------------------------------------------------------------\nexposures_desc.tab <- bmi_wide %>%\nselect(one_of(exposures)) %>%\nCreateTableOne(data = ., includeNA = TRUE) %>%\nprint(., quote = FALSE, noSpaces = TRUE, printToggle = FALSE, format = \"f\",\n         test = FALSE, varLabels = TRUE)\n\nwrite.csv(exposures_desc.tab, \n    file = paste0(\"Exposures descriptives_\", cohort, \".csv\"))\n\n\n## ---- Confounders ------------------------------------------------------------\nconfounders_desc.tab <- bmi_wide %>%\nselect(one_of(confounders_vec)) %>%\nCreateTableOne(data = ., includeNA = TRUE) %>%\nprint(., quote = FALSE, noSpaces = TRUE, printToggle = FALSE, format = \"f\",\n         test = FALSE, varLabels = TRUE)\n\nwrite.csv(confounders_desc.tab, \n    file = paste0(\"Confounders descriptives_\", cohort, \".csv\"))\n\n## ---- Outcome ----------------------------------------------------------------\noutcomes_ref <- c(\"bmi_raw_0_2\", \"bmi_int_z_0_2\", \"bmi_age_z_0_2\",\n                  \"bmi_raw_3_4\", \"bmi_int_z_3_4\", \"bmi_age_z_3_4\",\n                  \"bmi_raw_5_8\", \"bmi_int_z_5_8\", \"bmi_age_z_5_8\",\n                  \"bmi_raw_9_14\", \"bmi_int_z_9_14\", \"bmi_age_z_9_14\",\n                  \"bmi_raw_15_18\", \"bmi_int_z_15_18\", \"bmi_age_z_15_18\")\n             \noutcomes_desc.tab <- bmi_wide %>%\nselect(one_of(outcomes_ref)) %>%\nCreateTableOne(data = ., includeNA = TRUE) %>%\nprint(., quote = FALSE, noSpaces = TRUE, printToggle = FALSE, format = \"f\",\n         test = FALSE, varLabels = TRUE)\n\nwrite.csv(outcomes_desc.tab, \n    file = paste0(\"Outcomes descriptives_\", cohort, \".csv\"))\n\n\n################################################################################\n# 7. Run models  \n################################################################################\n\nregs <- list()\n\nfor(i in exposures){\n\nregs[[i]] <- bmiPoc(exp = i, conf = confounders[[i]], coh = cohort)\n\n}\n\ncombined_output <- do.call(\"rbind\", regs)\n\nsave(combined_output, \n    file = paste0(\"Combined output for all exposures_\", cohort))",
