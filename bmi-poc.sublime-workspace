			"contents": "################################################################################\n## Project: bmi-poc\n## Script purpose: Functions for analysis and tables\n## Date: 29th October 2019\n## Author: Tim Cadman\n## Email: t.cadman@bristol.ac.uk\n################################################################################\n\n################################################################################\n# 1. lmWrap\n################################################################################\n\n## This is just a wrapper to neaten up the lm code by specifying the data\n\nlmWrap <- function(formula){\n\nlm(formula, data = bmi_wide)    \n}\n\n\n################################################################################\n# 2. rlapply  \n################################################################################\n\n## A wrapper for lapply which rbinds the output\nrlapply <- function(...){\n\ndo.call(rbind, lapply(...))\n\n}\n\n\n################################################################################\n# 3. bmiTab \n################################################################################\n\nx <- results$age_9_14$raw$direct$unadjusted\ntab_exp <- \"edu_m_0\"\ntab_coh <- \"ALSPAC\"\ntab_conf <- \"sex\"\n\n# This takes the output of a nested list of models and puts them into a table.\nbmiTab <- function(x, tab_exp, tab_coh, tab_conf){\n\nif(length(x) == 0 || is.na(x)){\n\nreturn(NULL)\n\n}\n\nout <- data.frame(matrix(NA, ncol = 10, \n                  nrow = length(attributes(x$coefficients)$names)))\ncolnames(out) <- c(\"outcome\", \"exposure\", \"predictor\", \"n\", \"adjusted\", \n                   \"direct\", \"estimate\", \"se\", \"cohort\", \"formula\")\nform <- toString(formula(x))\nout$outcome <- colnames(x$model)[1]\nout$exposure <- tab_exp\nout$predictor <- attributes(x$coefficients)$names\nout$n <- length(x$residuals)\nout$adjusted <- ifelse(grepl(tab_conf[1], form) == TRUE, \n                       \"adjusted\", \"unadjusted\")\nout$direct <- ifelse(\n    lengths(regmatches(form, gregexpr(\"bmi\", form))) == 1, \"total\", \"direct\")\nout$estimate <- x$coefficients\nout$se <- sqrt(diag(vcov(x)))\nout$cohort <- tab_coh\nout$formula <- form\n\nreturn(out)\n}\n\n\n################################################################################\n# 3. bmiPoc  \n################################################################################\n\n# This runs all of the required models, extracts the required information and\n# puts the output in a table\n\nbmiPoc <- function(exp, conf, coh){\n\nprint(exp)\n\nresults <- list()\n\nif(exp %in% colnames(bmi_wide) == FALSE ||\n   all(is.na(bmi_wide[exp])) == TRUE) {\n\n    stop(paste0(\"Error: variable \", \"'\", exp, \"'\", \"not found, aborting\"))\n   }\n\nbmi_type <- c(\"raw\", \"int_z\", \"age_z\")\n\n## ---- First age category -----------------------------------------------------\n\n# I could do this in endless for loops but it gets quite hard to read. What I'm\n# looping over here are the three different forms of BMI outcome.\n\n## ---- Age 0 - 2 --------------------------------------------------------------\nfor(i in bmi_type){\n\ntot_unadj <- paste0(\"bmi_\", i, \"_0_2 ~ \", exp)\ntot_adj <- paste0(\"bmi_\", i, \"_0_2 ~ \", exp, \" + \", conf)\n\n## Total\nresults$age_0_2[[i]]$total$unadjusted <- lmWrap(formula = tot_unadj)\n\nresults$age_0_2[[i]]$total$adjusted <- lmWrap(formula = tot_adj)\n\n## Direct\nresults$age_0_2[[i]]$direct$unadjusted <- NA\nresults$age_0_2[[i]]$direct$adjusted <- NA\n}\n\n\n## ---- Ages 3 - 4 -------------------------------------------------------------\nfor(i in bmi_type){\n\ntot_unadj <- paste0(\"bmi_\", i, \"_3_4 ~ \", exp)\ntot_adj <- paste0(\"bmi_\", i, \"_3_4 ~ \", exp, \" + \", conf)\n\ndir_unadj <- paste0(\"bmi_\", i, \"_3_4 ~ \", exp, \" + \", \"bmi_\", i, \"_0_2\")\ndir_adj <- paste0(\"bmi_\", i, \"_3_4 ~ \", exp, \" + \", \"bmi_\", i, \"_0_2\", \n                    \" + \", conf)\n\n## Total\nresults$age_3_4[[i]]$total$unadjusted <- lmWrap(formula = tot_unadj)\n\nresults$age_3_4[[i]]$total$adjusted <- lmWrap(formula = tot_adj)\n\n## Direct\nresults$age_3_4[[i]]$direct$unadjusted <- lmWrap(formula = dir_unadj)\nresults$age_3_4[[i]]$direct$adjusted <- lmWrap(formula = dir_adj)\n}\n\n\n## ---- Ages 5 - 8 -------------------------------------------------------------\nfor(i in bmi_type){\n\ntot_unadj <- paste0(\"bmi_\", i, \"_5_8 ~ \", exp)\ntot_adj <- paste0(\"bmi_\", i, \"_5_8 ~ \", exp, \" + \", conf)\n\ndir_unadj <- paste0(\"bmi_\", i, \"_5_8 ~ \", exp, \" + \", \"bmi_\", i, \"_0_2\", \n                    \" + \", \"bmi_\", i, \"_3_4\")\ndir_adj <- paste0(\"bmi_\", i, \"_5_8 ~ \", exp, \" + \", \"bmi_\", i, \"_0_2\", \n                    \" + \", \"bmi_\", i, \"_3_4\", \" + \", conf)\n\n## Total\nresults$age_5_8[[i]]$total$unadjusted <- lmWrap(formula = tot_unadj)\n\nresults$age_5_8[[i]]$total$adjusted <- lmWrap(formula = tot_adj)\n\n## Direct\nresults$age_5_8[[i]]$direct$unadjusted <- lmWrap(formula = dir_unadj)\nresults$age_5_8[[i]]$direct$adjusted <- lmWrap(formula = dir_adj)\n}\n\n\n## ---- Ages 9 - 14 ------------------------------------------------------------\nfor(i in bmi_type){\n\ntot_unadj <- paste0(\"bmi_\", i, \"_9_14 ~ \", exp)\ntot_adj <- paste0(\"bmi_\", i, \"_9_14 ~ \", exp, \" + \", conf)\n\ndir_unadj <- paste0(\"bmi_\", i, \"_9_14 ~ \", exp, \" + \", \"bmi_\", i, \"_0_2\", \n                    \" + \", \"bmi_\", i, \"_3_4\", \" + \", \"bmi_\", i, \"_5_8\")\n\n\ndir_adj <- paste0(\"bmi_\", i, \"_9_14 ~ \", exp, \" + \", \"bmi_\", i, \"_0_2\", \n                    \" + \", \"bmi_\", i, \"_3_4\", \" + \", \n                    \" + \", \"bmi_\", i, \"_5_8\", \" + \", conf)\n\n## Total\nresults$age_9_14[[i]]$total$unadjusted <- lmWrap(formula = tot_unadj)\n\nresults$age_9_14[[i]]$total$adjusted <- lmWrap(formula = tot_adj)\n\n## Direct\nresults$age_9_14[[i]]$direct$unadjusted <- lmWrap(formula = dir_unadj)\nresults$age_9_14[[i]]$direct$adjusted <- lmWrap(formula = dir_adj)\n}\n\n\n## ---- Ages 15 - 18 -----------------------------------------------------------\nfor(i in bmi_type){\n\ntot_unadj <- paste0(\"bmi_\", i, \"_15_18 ~ \", exp)\ntot_adj <- paste0(\"bmi_\", i, \"_15_18 ~ \", exp, \" + \", conf)\n\ndir_unadj <- paste0(\"bmi_\", i, \"_15_18 ~ \", exp, \" + \", \"bmi_\", i, \"_0_2\", \n                    \" + \", \"bmi_\", i, \"_3_4\", \" + \", \"bmi_\", i, \"_5_8\", \n                    \" + \", \"bmi_\", i, \"_9_14\")\n\n\ndir_adj <- paste0(\"bmi_\", i, \"_15_18 ~ \", exp, \" + \", \"bmi_\", i, \"_0_2\", \n                    \" + \", \"bmi_\", i, \"_3_4\", \" + \", \n                    \" + \", \"bmi_\", i, \"_5_8\", \" + \", \n                    \" + \", \"bmi_\", i, \"_9_14\", \" + \", conf)\n\n## Total\nresults$age_15_18[[i]]$total$unadjusted <- lmWrap(formula = tot_unadj)\n\nresults$age_15_18[[i]]$total$adjusted <- lmWrap(formula = tot_adj)\n\n## Direct\nresults$age_15_18[[i]]$direct$unadjusted <- lmWrap(formula = dir_unadj)\nresults$age_15_18[[i]]$direct$adjusted <- lmWrap(formula = dir_adj)\n}\n\nout <- rlapply(results, function(x){\n          rlapply(x, function(x){\n              rlapply(x, function(x){\n                  rlapply(x, bmiTab, tab_exp = exp, tab_coh = coh, tab_conf = conf)})})})\n    \n\nreturn(out)\n\n}",
